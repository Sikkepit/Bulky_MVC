@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <tr>
        <th>Category Name</th>
        <th>Display Order</th>
    </tr>
    @foreach(var obj in Model.OrderBy(category=>category.DisplayOrder))
    {
        <tr>
            <td>@obj.Name</td>
            <td>@obj.DisplayOrder</td>
        </tr>   
    }
    
</table>
<a asp-controller="Category" asp-action="Create">New Category</a>